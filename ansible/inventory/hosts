backend ansible_host=backend ansible_port=22 ansible_ssh_pass="{{ lookup('env','ANSIBLE_SSH_PASS') }}" ansible_ssh_private_key_file=/root/.ssh/sample-key
frontend ansible_host=frontend ansible_port=22 ansible_ssh_pass="{{ lookup('env','ANSIBLE_SSH_PASS') }}" ansible_ssh_private_key_file=/root/.ssh/sample-key
